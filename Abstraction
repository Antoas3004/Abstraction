abstract class dog {
	abstract void bark();
}

class babydog extends dog {
	void bark() {
		System.out.println("barking");
	}
}

class cat extends dog {
	void bark() {
		System.out.println("no barking");
	}
}

public class Abstraction {

	public static void main(String[] args) {
		dog a = new babydog();
		a.bark();
		dog b = new cat();
		b.bark();
	}

}
